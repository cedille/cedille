
import Product.
import Functor.
import Sum.
import Unit.


%%% Base functor for natural numbers
NatF ◂ ★ ➔ ★ = λ X : ★ . Sum · Unit · X .


zF ◂ ∀ X : ★ . Unit ➔ NatF · X = in1 · Unit .
sF ◂ ∀ X : ★ . X ➔ NatF · X = in2 · Unit .



NatFF ◂ Functor · NatF =
  Λ X . Λ Y . λ f . λ u . u.1 · (NatF · Y) (zF · Y) (λ x . sF · Y (f x)) .

Flaw1 ◂ FunctorLaw1 · NatF NatFF =
  Λ X . Λ Y . Λ f . λ u . Λ p .
    θ<u> (ind-Sum · Unit · X u) (λ a . β) (λ b . ρ+ (p b) - β) .
    
