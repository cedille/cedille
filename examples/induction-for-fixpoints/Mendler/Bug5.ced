

Unit ◂ ★ = ∀ X : ★ . X ➔ X.
unit ◂ Unit = Λ X . λ x . x.

bug ◂ ∀ P : Unit ➔ ★ . 
      (Π u : Unit. P u) ➔ 
      Π q : Unit .
      P q =  Λ P . λ f . λ q .      
    let unit' ◂ Unit = q in  
    let unit'' ◂ Unit = unit' in 
     (f unit'').


nobug ◂ ∀ P : Unit ➔ ★ . 
      (Π u : Unit. P u) ➔ 
      Π q : Unit .
      P q =  Λ P . λ f . λ q .      
    let unit' ◂ Unit = q in  
%   let unit'' ◂ Unit = unit' in 
    let prf ◂ P unit' = f unit' in prf .
